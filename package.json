{
  "name": "northstar-json-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev-be dev-fe",
    "dev-fe": "doppler run -c local -- vite client",
    "dev-be": "doppler run -c local -- nodemon --exitcrash server/src/main/server.ts",
    "dev-hosted-be": "doppler run -c dev -- nodemon --exitcrash server/src/main/server.ts",
    "dev-db": "./.dev_db_start.sh",
    "dev-db-start": "./.dev_db_start.sh",
    "dev-db-stop": "npx supabase stop",
    "dev-db-reset": "npx supabase db reset",
    "dev-db-env": "cp .supa .env",
    "build": "npm run build-fe && npm run build-be",
    "build-fe": "tsc --p client/tsconfig.json && vite build client",
    "build-be": "tsc --p server/tsconfig.json && node server/bundler.mjs",
    "start": "node dist/server.js",
    "lint": "npm run lint-fe && npm run lint-be",
    "lint-fe": "eslint client/src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-be": "eslint server/src --ext ts,tsx --report-unused-disable-directives --max-warnings 1000",
    "preview": "cd client && vite preview",
    "test": "jest --ci --runInBand --collectCoverage=true",
    "counties-script": "doppler run -c local --  node scripts/load-counties-script/loadCountyData.js",
    "worker": "node dist/worker.js",
    "local-worker": "doppler run -c local -- node dist/worker.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/lab": "^5.0.0-alpha.131",
    "@mui/material": "^5.13.1",
    "@mui/x-data-grid": "^6.14.0",
    "@mui/x-date-pickers": "^6.5.0",
    "@sendgrid/mail": "^7.7.0",
    "@supabase/supabase-js": "^2.24.0",
    "axios": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.11.7",
    "doppler": "^1.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-promise": "^11.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "stripe": "^12.8.0",
    "ts-mockito": "2.6.1",
    "tsyringe": "^4.8.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.2.5",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.0.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "concurrently": "^8.0.1",
    "esbuild": "^0.17.19",
    "esbuild-decorators": "^1.0.0",
    "esbuild-plugin-tsc": "^0.4.0",
    "eslint": "^8.14.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "supabase": "^1.88.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.3.2"
  }
}
